  "version": "0.2.0",
  "configurations": [
    {
      "name": "espresso for web",
      "type": "dart",
      "request": "launch",
      "program": "espresso/lib/main.dart",
      "args": ["-d", "chrome"]
    },
    {
      "name": "espresso",
      "cwd": "espresso",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "espresso (profile mode)",
      "cwd": "espresso",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "espresso (release mode)",
      "cwd": "espresso",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
